      </implementation>
    </group>
    <group arch="Linux-i386">
      <implementation id="sha1new=75b487d7e1b7d179dc845e1f393afdf997a4b0a8" released="2008-08-16" stability="stable" version="7.15.5-1">
        <archive href="http://ftp.debian.org/debian/pool/main/c/curl/libcurl3-gnutls_7.15.5-1etch1_i386.deb" size="163196"/>
      </implementation>
    </group>
  </group>
