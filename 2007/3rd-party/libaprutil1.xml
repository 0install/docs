    </requires>
    <requires interface="http://0install.net/2007/3rd-party/libpq4.xml">
      <environment insert="usr/lib" name="LD_LIBRARY_PATH"/>
    </requires>
    <requires interface="http://0install.net/2007/3rd-party/libldap2.xml">
      <environment insert="usr/lib" name="LD_LIBRARY_PATH"/>
    </requires>
    <requires interface="http://0install.net/2007/3rd-party/libexpat1.xml">
      <environment insert="usr/lib" name="LD_LIBRARY_PATH"/>
    </requires>
  </group>
