<?xml version="1.0" ?>
<?xml-stylesheet type='text/xsl' href='interface.xsl'?>
<interface uri="http://0install.net/tests/Drupal-Shoutbox.xml" xmlns="http://zero-install.sourceforge.net/2004/injector/interface">
  <name>Drupal-Shoutbox</name>
  <summary>visitors can quickly post short messages</summary>

  <!-- Optionally, uncomment this to specify the program's homepage: -->
  <homepage>http://drupal.org/project/shoutbox</homepage>
  <description>
The Shoutbox module provides a block where visitors can quickly post short
messages.</description>
  <group license="OSI Approved :: GNU General Public License v2">
    <environment insert="shoutbox" name="drupal-modules"/>

    <group>
      <!-- Drupal 7.x versions -->
      <requires interface="http://0install.net/tests/Drupal.xml">
	<version before="8" not-before="7"/>
      </requires>

      <implementation id="sha1new=df327289c2be387c6e3e7ff3eb0bb1305babc9b1" released="2011-11-13" version="1.0-pre2">
	<archive href="http://ftp.drupal.org/files/projects/shoutbox-7.x-1.0-alpha2.tar.gz" size="35838" type="application/x-compressed-tar"/>
      </implementation>
    </group>

    <group>
      <!-- Drupal 6.x versions -->
      <requires interface="http://0install.net/tests/Drupal.xml">
	<version before="7" not-before="6"/>
      </requires>

      <implementation id="sha1new=5a0d908fc53e4467937104d6b4d46901d89129f4" released="2011-11-13" version="2.7">
	<archive href="http://ftp.drupal.org/files/projects/shoutbox-6.x-2.7.tar.gz" size="34093" type="application/x-compressed-tar"/>
      </implementation>
    </group>
  </group>
</interface>
